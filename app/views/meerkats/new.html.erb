 <h1 class ="new-meerkat">Create a new Meerkat </h1>

<%= simple_form_for (@meerkat), html: { autocomplete: 'off' } do |f| %>
<div class="form-inputs">
  <%= f.input :name %>
  <%= f.input :specialty %>
  <%= f.input :age %>
  <%= f.input :price %>
  <%= f.input :location %>
  <%= f.input :description %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit %>
</div>
<% end %>
